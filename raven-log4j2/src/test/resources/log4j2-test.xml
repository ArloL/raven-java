<?xml version="1.0" encoding="UTF-8"?>
<configuration status="all" packages="org.apache.logging.log4j.core,net.kencochrane.raven.log4j2">
    <appenders>
        <Console name="STDOUT" target="SYSTEM_OUT"/>
    </appenders>

    <loggers>
        <root level="all">
            <appender-ref ref="STDOUT"/>
        </root>
    </loggers>
</configuration>
